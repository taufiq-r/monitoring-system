      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "sum(time() - node_boot_time_seconds) by (instance)",
          "format": "table",
          "instant": true,
          "refId": "D"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "node_memory_MemTotal_bytes",
          "format": "table",
          "instant": true,
          "refId": "B"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "count(node_cpu_seconds_total{mode=\"system\"}) by (instance)",
          "format": "table",
          "instant": true,
          "refId": "C"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "node_load5",
          "format": "table",
          "instant": true,
          "refId": "L"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "(1 - avg(rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) by (instance)) * 100",
          "format": "table",
          "instant": true,
          "refId": "F"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
          "format": "table",
          "instant": true,
          "refId": "G"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "max((node_filesystem_size_bytes{mountpoint=\"/\"} - node_filesystem_free_bytes{mountpoint=\"/\"}) * 100 / node_filesystem_size_bytes{mountpoint=\"/\"}) by (instance)",
          "format": "table",
          "instant": true,
          "refId": "E"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "max(rate(node_disk_read_bytes_total[5m])) by (instance)",
          "format": "table",
          "instant": true,
          "refId": "H"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "max(rate(node_disk_written_bytes_total[5m])) by (instance)",
          "format": "table",
          "instant": true,
          "refId": "I"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "max(rate(node_network_receive_bytes_total{device!~\"lo|docker.*|veth.*\"}[5m]) * 8) by (instance)",
          "format": "table",
          "instant": true,
          "refId": "J"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "max(rate(node_network_transmit_bytes_total{device!~\"lo|docker.*|veth.*\"}[5m]) * 8) by (instance)",
